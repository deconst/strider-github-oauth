  "scripts": {
    "test": "mocha -R spec --recursive test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smashwilson/strider-github-auth.git"
  },
  "keywords": [
    "strider",
    "plugin",
    "template"
  ],
  "author": "Ash Wilson <kyle@bitwit.ca>",
  "license": "MIT",
  "dependencies": {
    "passport": "^0.3.2",
    "passport-github2": "^0.1.9"
